  // file1.cpp
  // Contains the main function
  // Demo file COSC350 Lab1
  //
  // Will Townsend
  // Created: 02/03/2022
  // Current: 02/03/2022

  #include <iostream>
  #include "file2.h"

  int main()
  {
    using namespace std;
    int i = 3;
    int sq;

    sq = SquareIt(i);
    cout << "Square of " << i << " is " << sq << endl;
    return 0;
  }

  //file2.h
  //Demo file COSC350 Lab1
  // Declaration of SquareIt function
  //
  //Will Townsend
  //Created: 02/03/2022
  //Current: 02/03/2022
  #ifndef FILE2_H
  #define FILE2_H

    int SquareIt(int);

  #endif

  //file2.cpp
  //Demo file COSC350 Lab1
  //Definition of SquareIt function
  //
  //Will Townsend
  //Created: 02/03/2022
  //Current: 02/03/2022

  #include "file2.h"

  int SquareIt(int x){
          return x * x;
  }

